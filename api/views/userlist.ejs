<!-- index.ejs -->
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Star Wars Quote App</title>
  </head>

  <body>
    <h2> Quotes </h2>

    <ul class="quotes">
      <!-- Loop through quotes -->
      <% for(var i = 0; i < quotes.length; i++) 
      {%>
        <li class="quote">
          
          <span><%= quotes[i].username %></span>:
          <span><%= quotes[i].email %></span>:
          
          <span><%= quotes[i].password %></span>
        </li>
      <% } %>


  </body>
</html>